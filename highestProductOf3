const highestProduct = (arr) => {
  let highest;
  
  for (let i = 0; i < arr.length; i++) {
    for (let j = i + 1; j < arr.length; j++) {
      for (let k = j + 1; k < arr.length; k++) {
        let curr = arr[i] * arr[j] * arr[k]
        if (curr > highest || highest === undefined) {
          highest = curr
        }
      }
    }
  }
  
  return highest;
}

highestProduct([1,2,-3,-4,5,6])
